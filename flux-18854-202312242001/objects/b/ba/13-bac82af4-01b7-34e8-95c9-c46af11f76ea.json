  "rdfs:comment": {
    "de": [
      "Weinbar mit 70 Weinen französischer und ausländischer Erzeugung. Mittagsmenü und Abendgericht. Lauschiger Ort mit Stehtischen, Tischen und Sitzbänken. Kapazität: 60 Plätze und 20 auf der Terrasse."
    ],
    "en": [
      "Wine bar with a choice of 70 French and foreign wines. Restaurant service available at midday with snacks in the evenings. Cosy dining area with standing tables, seated tables and benches. Capacity: 60 indoors and 20 on the terrace."
    ],
    "it": [
      "Wine bar, 70 etichette francesi e straniere. Formula ristorante a pranzo e piatti misti a cena. Spazio informale per mangiare in piedi o seduti su panche. Capacità: 60 posti e 20 all'aperto."
    ],
    "fr": [
      "Bar à vins, 70 références de productions françaises et étrangères. Formule resto le midi et assiette le soir. Espace cosy avec mange-debouts, tables, banquettes. Capacité : 60 places et 20 en terrasse."
    ],
    "nl": [
      "Wijnbar, 70 referenties van Franse en buitenlandse producties. Restaurantformule tussen de middag en een bord 's avonds. Gezellige ruimte met tafels en banken. Capaciteit: 60 zitplaatsen en 20 op het terras."
    ],
    "es": [
      "Bar de vinos con 70 referencias de productos franceses y extranjeros. Menús a mediodía y bandejas de aperitivos por la noche. Espacio acogedor con mesas altas, mesas bajas y bancos. Capacidad: 60 plazas y 20 en terraza."
    ]
  },
