  "rdfs:comment": {
    "de": [
      "Weinstube, Weinhandlung, Restaurant. Großzügig Wein und gute Produkte teilen. Ausschließlich lokal versorgt mit Gemüse, Geflügel, Eiern, ... Große Auswahl an Weinen aus dem Beaujolais."
    ],
    "en": [
      "Wine bar, wine shop, restaurant. Share generously the wine, the good products. Exclusively supplied locally for vegetables, poultry, eggs,. . . Great selection of Beaujolais wines."
    ],
    "it": [
      "Wine bar, enoteca, ristorante. Condividere il vino e i buoni prodotti con generosità. Verdure, pollame, uova, ecc. di provenienza esclusivamente locale. Ampia selezione di vini del Beaujolais."
    ],
    "fr": [
      "Bar à vin, caviste, restaurant. Partager avec générosité le vin, les bons produits. Exclusivement approvisionné en local pour les légumes, volailles, oeufs,... Grande sélection de vins du Beaujolais."
    ],
    "nl": [
      "Wijnbar, wijnwinkel, restaurant. Wijn en goede producten delen met gulheid. Uitsluitend lokale groenten, gevogelte, eieren, enz. Grote selectie van Beaujolais wijnen."
    ],
    "es": [
      "Bar de vinos, tienda de vinos, restaurante. Compartir el vino y los buenos productos con generosidad. Exclusivamente verduras, aves de corral, huevos, etc. de origen local. Gran selección de vinos de Beaujolais."
    ]
  },
